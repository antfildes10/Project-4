{
  "blank_line_after_tag": "load,extends,include",
  "blank_line_before_tag": "block,endblock",
  "close_void_tags": true,
  "custom_blocks": "",
  "custom_html": "",
  "exclude": ".venv,venv,env,staticfiles,node_modules",
  "extend_exclude": "",
  "format_attribute_template_tags": true,
  "format_css": true,
  "format_js": true,
  "ignore": "H006,H030,H031",
