# Prettier ignore file for KartControl project

# Django templates - Prettier doesn't understand Django template syntax
templates/**/*.html

# Python files - use flake8/black instead
*.py

# Django migrations
**/migrations/*.py

# Virtual environment
venv/
env/

# Static files (collected)
staticfiles/

# Database
*.sqlite3
db.sqlite3

# Node modules
node_modules/

# Coverage reports
htmlcov/
.coverage

# IDE
.vscode/
.idea/
*.swp
*.swo
